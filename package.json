{
  "name": "schema-org-adapter",
  "version": "5.0.3",
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "Fast, simple & flexible API for the Schema.org Vocabulary (and vocabulary extensions!) for Node and Browsers",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "buildAll": "npm run-script buildTs && npm run-script buildDist && npm run-script buildDistMinified",
    "watchTs": "tsc -b src --watch",
    "buildTs": "tsc -b src",
    "buildDist": "browserify lib/index.js -s SOA | derequire -cm --toplevel > dist/schema-org-adapter.js",
    "buildDistMinified": "browserify lib/index.js -s SOA -g [uglifyify] | derequire -cm --toplevel > dist/schema-org-adapter.min.js",
    "test:badges": "npm run-script testSoaRepo && npm run-script badges",
    "testSoaRepo": "jest",
    "testSchemaRepo": "COMMIT=9a3ba4684a21c88506d0fed5ea37e5c6bbb115a1 jest",
    "badges": "jest-coverage-badges input './docu/coverage/coverage-summary.json' output './docu/coverage'",
    "eslint-check": "eslint src/*.ts tests/*.ts",
    "eslint-repair": "eslint --fix src/*.ts tests/*.ts",
    "prettier-check": "prettier --check src/*.ts tests/*.ts",
    "prettier-repair": "prettier --write src/*.ts tests/*.ts",
    "genTypeDoc": "typedoc",
    "watchTypeDoc": "typedoc --watch"
  },
  "author": {
    "name": "Omar J. A. Holzknecht for STI Innsbruck",
    "url": "https://semantify.it"
  },
  "license": "Apache-2.0",
  "homepage": "https://semantifyit.github.io/schema-org-adapter/",
  "keywords": [
    "schema.org",
    "adapter",
    "schema",
    "org",
    "converter",
    "sdo-adapter",
    "semantic web"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/semantifyit/schema-org-adapter"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "jsonld": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/jsonld": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "browserify": "^17.0.0",
    "derequire": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.0",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.10",
    "typedoc-plugin-merge-modules": "^3.1.0",
    "typescript": "^4.5.2",
    "uglifyify": "^5.0.2"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "node",
    "testTimeout": 30000,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.ts"
    ],
    "coverageDirectory": "docu/coverage",
    "coverageReporters": [
      "json-summary"
    ],
    "globals": {
      "ts-jest": {
        "isolatedModules": true,
        "tsconfig": "tests/tsconfig.json"
      }
    }
  },
  "files": [
    "lib/*",
    "dist/*",
    "README.md",
    "History.md",
    "LICENSE",
    "package.json"
  ]
}
